pipeline{
    agent any
    tools{
        maven "maven3.9.5"
    }
    stages{
        stage('clonecode4scm'){
            steps{
                sh "echo 'this is declarative pipeline'"
                git branch: 'nov18', url: 'https://github.com/muller-tech/liontech-online-library-web-app.git'

            }
        }
    }























}